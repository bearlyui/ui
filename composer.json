{
    "name": "bearly/ui",
    "description": "A set of grizzly UI components to drop into your app.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "laravel/prompts": "^0.3.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "livewire/livewire": "^3.4",
        "orchestra/testbench": "^9.5",
        "orchestra/testbench-dusk": "^9.9"
    },
    "autoload": {
        "psr-4": {
            "Bearly\\Ui\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Austen Cameron",
            "email": "austen.cameron@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Bearly\\Ui\\BearUiProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bearly\\Ui\\Tests\\": "tests/"
        }
    }
}
